<target name="help">
    <echo message="Please run: $ ant -v -projecthelp"/>
</target>

<target depends="init" name="buildcspom">
        <echo message="${ant.project.name}: ${ant.file}"/>
        <javac destdir="bin" optimize="on">
            <src path="src/cspom"/>
        </javac>
</target>

<target depends="clean,buildcspom" name="cspom">
        <jar destfile="cspom.jar" basedir="bin" manifest="cspom.manifest" index="true">
        	<file file="src/cspom/predicate/predefinedFunctions.js" />
        </jar>
</target>